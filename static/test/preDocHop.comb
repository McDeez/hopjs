<div class="span9">
  <span>
  <select id="preDocTest">
    <option value="All">All</option>
    <% for(var testCase in Hop.TestCases){%>
      <option value="<%=testCase%>"><%=testCase%></option> 
    <% } %>
  </select>
  <button onClick='Hop.TestCase.runTest($("#preDocTest").val())'/>Run</button>
  </span>
</div>

